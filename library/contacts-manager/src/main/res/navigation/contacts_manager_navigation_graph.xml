<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contacts_manager_navigation_graph"
    app:startDestination="@id/chooseContactsFragment">

    <fragment
        android:id="@+id/chooseContactsFragment"
        android:name="com.planner.library.contacts_manager.ChooseContactsFragment"
        android:label="fragment_choose_contacts"
        tools:layout="@layout/fragment_choose_contacts">

        <action
            android:id="@+id/action_addTripFragment_to_contacts_manager_navigation_graph"
            app:destination="@+id/chooseContactsFragment" />
        <argument
            android:name="selectedContacts"
            app:argType="com.planner.library.contacts_manager.Contact[]"
            app:nullable="true" />
    </fragment>
</navigation>